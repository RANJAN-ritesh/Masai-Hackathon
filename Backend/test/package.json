{
  "name": "hackathon-admin-tests",
  "version": "1.0.0",
  "description": "Test suite for admin functionality",
  "main": "admin.test.js",
  "scripts": {
    "test": "jest",
    "test:admin": "jest admin.test.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testTimeout": 30000,
    "verbose": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  }
} 